<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sitecreative.yaposhka64" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>Япошка</name>
    <description>
        Мобильное приложение сети ресторатов Япошка
    </description>
    <author email="site-creative@mail.ru" href="https://site-creative.ru/">
        Веб студия Site-Creative
    </author>
    <content src="index.html" />
    <icon src="/public/assets/branding/logo_575x575.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="public/assets/branding/android/mipmap-ldpi/ic_launcher.png" />
    <icon density="mdpi" src="public/assets/branding/android/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="public/assets/branding/android/mipmap-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="public/assets/branding/android/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="public/assets/branding/android/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="public/assets/branding/android/mipmap-xxxhdpi/ic_launcher.png" />
    <splash height="960" src="public/assets/branding/ios/splash/Default@2x~iphone.png" width="640" />
    <splash height="768" src="public/assets/branding/ios/splash/ipad landscape x1.png" width="1024" />
    <splash height="1536" src="public/assets/branding/ios/splash/ipad landscape x2.png" width="2048" />
    <splash height="480" src="public/assets/branding/ios/splash/ipad portrait x1.png" width="320" />
    <splash height="1136" src="public/assets/branding/ios/splash/ipad Retina 4.png" width="640" />
    <splash height="1024" src="public/assets/branding/ios/splash/iphone portrait ios 5,6 x1-1.png" width="768" />
    <splash height="1536" src="public/assets/branding/ios/splash/iphone portrait ios 5,6 x2-1.png" width="2008" />
    <splash height="2008" src="public/assets/branding/ios/splash/iphone portrait ios 5,6 x2.png" width="1536" />
    <splash height="1024" src="public/assets/branding/ios/splash/iphone portrait x1.png" width="768" />
    <splash height="1538" src="public/assets/branding/ios/splash/iphone portrait x2_2.png" width="2048" />
    <splash height="2048" src="public/assets/branding/ios/splash/iphone portrait x2.png" width="1538" />
    <splash height="1125" src="public/assets/branding/ios/splash/iPhone Xs_2.png" width="2436" />
    <splash height="2436" src="public/assets/branding/ios/splash/iPhone Xs.png" width="1125" />
    <splash height="1334" src="public/assets/branding/ios/splash/Retina HD 4.7.png" width="750" />
    <splash height="1242" src="public/assets/branding/ios/splash/Retina HD 5.5_2.png" width="2208" />
    <splash height="2208" src="public/assets/branding/ios/splash/Retina HD 5.5.png" width="1242" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="none" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
</widget>
